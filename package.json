{
  "name": "ai-projects",
  "version": "1.0.0",
  "description": "A full-stack of AI Projects with AI agents and project management",
  "main": "index.js",
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "start": "turbo start --filter=backend",
    "start:prod": "turbo start:prod --filter=backend",
    "db:migrate": "turbo db:migrate --filter=backend",
    "db:seed": "turbo db:seed --filter=backend",
    "railway:build": "turbo build",
    "railway:start": "./start-railway.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "install:all": "pnpm install"
  },
  "keywords": [
    "ai",
    "projects",
    "chat",
    "full-stack",
    "ai"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "turbo": "^2.5.6"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  }
}